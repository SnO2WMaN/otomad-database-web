{
  "name": "@otomadb/web",
  "private": true,
  "scripts": {
    "prepare": "lefthook install",
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "fmt": "pnpm run lint:prettier --write",
    "lint:eslint": "eslint . --ext \".js,.jsx,.ts,.tsx\"",
    "lint:prettier": "prettier --check .",
    "lintfix": "run-s 'lintfix:*' 'fmt'",
    "lintfix:eslint": "pnpm run lint:eslint --fix",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "pnpm run codegen --watch",
    "schema:validate": "graphql-inspector validate \"./**/*.{ts,tsx}\" ./schema.graphql",
    "schema:diff": "graphql-inspector diff ./schema.graphql",
    "schema:introspect": "graphql-inspector introspect --write schema.graphql",
    "storybook": "sb dev -p 6006 --no-open",
    "storybook:build": "sb build",
    "storybook:preview": "serve storybook-static",
    "vercel:web": "run-s 'codegen' 'build'",
    "vercel:storybook": "run-s 'codegen' 'storybook:build'",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "engines": {
    "node": "18"
  },
  "packageManager": "pnpm@8.11.0",
  "dependencies": {
    "@auth0/auth0-react": "^2.2.3",
    "@auth0/nextjs-auth0": "^3.3.0",
    "@emotion/css": "^11.11.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^2.0.18",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "3.3.2",
    "@next/mdx": "^14.0.3",
    "@svgr/webpack": "^8.1.0",
    "@urql/exchange-auth": "^2.1.6",
    "@urql/exchange-graphcache": "^6.3.3",
    "client-only": "^0.0.1",
    "clsx": "^2.0.0",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^2.30.0",
    "graphql": "^16.8.1",
    "graphql-request": "6.1.0",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.46.2",
    "react-use": "^17.4.0",
    "sass": "^1.69.5",
    "server-only": "^0.0.1",
    "urql": "^4.0.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/urql-introspection": "3.0.0",
    "@graphql-inspector/cli": "4.0.2",
    "@graphql-typed-document-node/core": "3.2.0",
    "@storybook/addon-actions": "7.5.3",
    "@storybook/addon-docs": "7.5.3",
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/cli": "7.5.3",
    "@storybook/nextjs": "7.5.3",
    "@storybook/react": "7.5.3",
    "@storybook/testing-library": "0.2.2",
    "@tailwindcss/container-queries": "0.1.1",
    "@tsconfig/node18": "18.2.2",
    "@types/mdx": "2.0.10",
    "@types/node": "18.18.13",
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.13.0",
    "@typescript-eslint/parser": "6.13.0",
    "@vitejs/plugin-react": "4.2.0",
    "autoprefixer": "10.4.16",
    "babel-loader": "9.1.3",
    "css-loader": "6.8.1",
    "encoding": "0.1.13",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "graphql-codegen-typescript-mock-data": "3.0.0",
    "lefthook": "1.5.4",
    "msw": "1.3.2",
    "msw-storybook-addon": "1.10.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "postcss-loader": "7.3.3",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "serve": "14.2.1",
    "style-loader": "3.3.3",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.2",
    "vitest": "0.34.6"
  },
  "peerDependencies": {
    "@parcel/watcher": "2.3.0"
  }
}
